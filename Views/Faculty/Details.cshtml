@model Homework06.Application.Models.Faculty
    <div class="card">
        <div class="card-header text-center">
            <h1>教师详情</h1>
        </div>
        <div class="card-body text-center">
            <table class="layui-table">
                <tbody>
                    <tr>
                        <td>Id</td>
                        <td>@Model.Id</td>
                    </tr>
                    <tr>
                        <td>名称</td>
                        <td>@Model.FirstName @Model.LastName</td>
                    </tr>
                    <tr>
                        <td>城市</td>
                        <td>@Model.City</td>
                    </tr>
                    <tr>
                        <td>国家</td>
                        <td>@Model.State</td>
                    </tr>
                    <tr>
                        <td>部门</td>
                        <td>@Model.Dept</td>
                    </tr>
                    <tr>
                        <td>等级</td>
                        <td>@Model.Rank</td>
                    </tr>
                    <tr>
                        <td>工资</td>
                        <td>@Model.Salary</td>
                    </tr>
                    <tr>
                        <td>上级</td>
                        <td>@Model.Supervisor</td>
                    </tr>
                    <tr>
                        <td>雇佣日期</td>
                        <td>@Model.HireDate</td>
                    </tr>
                    <tr>
                        <td>ZipCode</td>
                        <td>@Model.ZipCode</td>
                    </tr>
                </tbody>
            </table>
            @*<h4>Id：@Model.Id</h4>
            <h4>名称：@Model.FirstName @Model.LastName</h4>
            <h4>：@Model.City</h4>
            <h4>：@Model.State</h4>
            <h4>：@Model.Dept</h4>
            <h4>：@Model.Salary</h4>
            <h4>：@Model.Supervisor</h4>
            <h4>：@Model.HireDate</h4>
            <h4>：@Model.ZipCode</h4>*@
        </div>
        <div class="card-footer text-center">
            <a asp-action="Index" class="btn btn-primary">返回</a>
            <a asp-action="Update" asp-route-id="@Model.Id" class="btn btn-success">编辑</a>
            <a asp-action="CourseList" asp-route-id="@Model.Id" class="btn btn-success">辅导</a>
            <a asp-action="Delete" asp-route-id="@Model.Id" class="btn btn-danger">删除</a>
        </div>
    </div>