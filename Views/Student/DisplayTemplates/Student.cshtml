@model Homework06.Application.Models.Student
<tr>
    <td>@Model.Id</td>
    <td>@Model.FirstName @Model.LastName</td>
    <td>@Model.City</td>
    <td>@Model.State</td>
    <td>@Model.Class</td>
    <td>
        <div>
            <a asp-action="Details" asp-route-id="@Model.Id" class="btn btn-primary">详情</a>
            <a asp-action="Update" asp-route-id="@Model.Id" class="btn btn-success">编辑</a>
            <a asp-action="Delete" asp-route-id="@Model.Id" class="btn btn-danger">删除</a>
        </div>
    </td>
</tr>